profiles {

    standard {
        process.executor = 'local'
        docker.enabled = true
    }
}
process {

    withName: checkQ {
        cpus = 4
        memory = 4.GB
        container = 'djevtic/bwaandfastqc'
    }
    withName: checkVersion {
        cpus = 4
        memory = 4.GB
        container = 'djevtic/bwaandfastqc'
    }
}
